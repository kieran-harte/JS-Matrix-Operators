{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2020": true,
    "node": true,
    "jest": true
  },
  "extends": ["airbnb-base", "prettier", "plugin:node/recommended"],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 11
  },
  "plugins": ["prettier", "babel"],
  "rules": {
    "prettier/prettier": "error",
    "no-unused-vars": "warn",
    "func-names": "off",
    "no-empty-function": "error",
    "no-extra-parens": "warn"
  }
}
